<?xml version="1.0" encoding="UTF-8"?>
<CLISH_MODULE xmlns="http://clish.sourceforge.net/XMLSchema"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://clish.sourceforge.net/XMLSchema
                     http://clish.sourceforge.net/XMLSchema/clish.xsd">
    <!--=======================================================-->
    <VIEW name="main">
        <!--===================================================-->
        <COMMAND name="show syslog"
             help="Configure Syslog">
        </COMMAND>

        <COMMAND name="show syslog config"
             help="Configure Syslog">
            <ACTION>
                /etc/tf_clish/scripts/rsyslog/syslog_show.lua
            </ACTION>
        </COMMAND>

        <COMMAND name="show log"
             help="Configure Syslog">
        </COMMAND>

        <COMMAND name="show log config"
             help="Configure Syslog">
        </COMMAND>

        <COMMAND name="show log config all"
             help="Configure Syslog">
            <ACTION>
                /etc/tf_clish/scripts/rsyslog/log_show.lua "all"
            </ACTION>
        </COMMAND>

        <COMMAND name="show log config filter"
             help="Configure Syslog">
            <ACTION>
                /etc/tf_clish/scripts/rsyslog/log_show.lua "filter"
            </ACTION>
        </COMMAND>

        <COMMAND name="show log config action"
             help="Configure Syslog">
            <ACTION>
                /etc/tf_clish/scripts/rsyslog/log_show.lua "action"
            </ACTION>
        </COMMAND>

        <COMMAND name="show log config rule"
             help="Configure Syslog">
            <ACTION>
                /etc/tf_clish/scripts/rsyslog/log_show.lua "rule"
            </ACTION>
        </COMMAND>

    </VIEW>
    <!--=======================================================-->
</CLISH_MODULE>